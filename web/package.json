{
  "name": "@gamepulse-studio/web-sdk",
  "version": "2.0.21",
  "description": "GamePulse SDK for Web/Node.js - Cross-platform analytics for game developers",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rm -rf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "analytics",
    "gaming",
    "web",
    "typescript",
    "javascript",
    "nodejs",
    "browser",
    "cross-platform",
    "sdk",
    "gamepulse"
  ],
  "author": "GamePulse Team <support@gamepulse.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gamepulse/gamepulse-sdk.git",
    "directory": "packages/web"
  },
  "bugs": {
    "url": "https://github.com/gamepulse/gamepulse-sdk/issues"
  },
  "homepage": "https://github.com/gamepulse/gamepulse-sdk/tree/main/packages/web#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.19.15",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.0.0",
    "jest": "^29.6.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.9.2"
  }
}
